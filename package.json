  },
  "homepage": "https://github.com/j0flintking02/autoMart_api#readme",
  "dependencies": {
    "@hapi/joi": "^15.0.3",
    "babel-istanbul": "^0.12.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.14.0",
    "config": "^3.1.0",
    "dotenv": "^8.0.0",
    "env2": "^2.2.2",
    "express": "^4.17.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
